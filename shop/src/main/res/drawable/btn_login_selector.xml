<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/login_green_dark"/>
            <solid android:color="@color/login_blue_light"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0.5dp" android:color="@color/login_green_dark"/>
            <solid android:color="@color/login_blue_dark"/>
        </shape>
    </item>
</selector>
